{
  "name": "musicdblab",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "migrate-reset-postgres": "prisma migrate reset --schema=prisma-postgres/schema.postgres.prisma --force" ,
    "migrate-reset-mysql": "prisma migrate reset --schema=prisma-mysql/schema.mysql.prisma --force",
    "migrate-postgres": "npx prisma migrate dev --schema=prisma-postgres/schema.postgres.prisma --name init",
    "migrate-mysql": "npx prisma migrate dev --schema=prisma-mysql/schema.mysql.prisma --name init",
    "generate-postgres": "npx prisma generate --schema=prisma-postgres/schema.postgres.prisma",
    "generate-mysql": "npx prisma generate --schema=prisma-mysql/schema.mysql.prisma",
    "create-public2": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" prisma-postgres/create-public2-schema.ts --schema=prisma-postgres/schema.postgres.prisma",
    "predev": "npm run migrate-reset-postgres && npm run migrate-reset-mysql && npm run migrate-postgres && npm run migrate-mysql",
    "dev": "npm run create-public2 && npm run generate-postgres && npm run seed:postgres && npm run generate-mysql && npm run seed:mysql && next build",
    "prebuild": "npm run migrate-reset-postgres && npm run migrate-reset-mysql && npm run migrate-postgres && npm run migrate-mysql",
    "build": "npm run create-public2 && npm run generate-postgres && npm run seed:postgres && npm run generate-mysql && npm run seed:mysql && next build",
    "start": "npm run generate-postgres && npm run seed:postgres && npm run generate-mysql && npm run seed:mysql && next start",
    "lint": "next lint",
    "seed:postgres": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" prisma-postgres/seed.ts --schema=prisma-postgres/schema.prisma",
    "seed:mysql": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" prisma-mysql/seed.ts --schema=prisma-mysql/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "daisyui": "^5.0.6",
    "next": "15.2.3",
    "postcss": "^8.5.3",
    "prisma": "^6.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.0.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.2.3",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^4.0.14",
    "typescript": "^5"
  }
}
